[
    {
      "id": "bin_features",
      "component_type": "PayloadTransformer",
      "config": {
        "transformation_expression": "
          [
            .bins[] as $bin |
            {
              bin: $bin,
              features: .features | map(select(. >= $bin[0] and . < $bin[1]))
            }
          ]
        ",
        "validation_data": {
          "features": [1.0, 15.0, 25.0],
          "bins": [[0, 10], [10, 20], [20, 30]]
        }
      },
      "inputs": {
        "features": [3.2, 5.8, 12.1, 20.4, 25.9],
        "bins": [[0, 10], [10, 20], [20, 30]]
      }
    }
  ]